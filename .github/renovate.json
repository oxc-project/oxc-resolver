{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>Boshen/renovate"],
  "ignorePaths": ["**/node_modules/**", "**/fixtures/**"],
  "packageRules": [
    {
      "groupName": "ignored crates",
      "matchManagers": ["cargo"],
      "matchPackageNames": ["thiserror"],
      "enabled": false
    }
  ]
}
